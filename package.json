{
  "name": "dgeni-alive",
  "title": "Dgeni-Alive",
  "version": "0.2.1",
  "description": "Live doc generator on top of Dgeni",
  "main": "src/docgen.js",
  "scripts": {
    "clean": "rimraf ./coverage",
    "cover": "istanbul cover jasmine-node -- .",
    "travis": "istanbul cover jasmine-node -- . && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "test": "jasmine-node ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wingedfox/dgeni-alive.git"
  },
  "keywords": [
    "dgeni",
    "angular",
    "docs"
  ],
  "author": "Ilya WingedFox Lebedev <ilya@lebedev.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wingedfox/dgeni-alive/issues"
  },
  "homepage": "https://github.com/wingedfox/dgeni-alive#readme",
  "dependencies": {
    "bloomfilter": "~0.0.16",
    "bower": "~1.7.2",
    "bytebuffer": "~5.0.0",
    "catharsis": "~0.8.7",
    "dgeni": "~0.4.2",
    "dgeni-packages": "~0.11.1",
    "ent": "~2.2.0",
    "fs-extra": "~0.26.4",
    "http-server": "~0.8.5",
    "keyword-extractor": "~0.0.12",
    "lodash": "~4.0.0",
    "mime": "~1.3.4",
    "node-html-encoder": "~0.0.2",
    "opener": "~1.4.1",
    "q": "~1.4.1",
    "stemmer": "~0.1.4",
    "stringmap": "~0.2.2",
    "wiredep": "~3.0.0"
  },
  "devDependencies": {
    "coveralls": "~2.11.8",
    "istanbul": "~0.4.2",
    "jasmine-node": "~2.0.0",
    "rimraf": "~2.5.2"
  }
}
